<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
 http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="spring.core.collection.collectionexample"
		name="collection1">
		<property name="set">
			<set>
				<value>Satyam</value>
				<value>Mohan</value>
				<value>Arun</value>
			</set>
		</property>

		<property name="list">

			<list>
				<value>Arun</value>
				<value>Mohan</value>
				<value>Satyam</value>
				<value>Arun</value>
			</list>

		</property>

		<property name="map">
			<map>
				<entry key="1" value="mohan" />
				<entry key="2" value="salman" />
				<entry key="3" value="satyam" />
			</map>
		</property>
	</bean>
</beans>